<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>index2newfilm</from-outcome>
            <to-view-id>/newfilm.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>index2newdatastore</from-outcome>
            <to-view-id>/newdatastore.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>index2editfilm</from-outcome>
            <to-view-id>/editfilm.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/editfilm.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>editfilm2index</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/newfilm.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>newfilm2index</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
